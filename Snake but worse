    }
    if(a[n-1][k]=='.'){
    if(e=='.')
    {
        d++;
        a[n-d][k]='.';
        h=a[--x][y];
       
       
        a[n][k]=' ';
    }
    if(a[n][k-1]=='.') //neighbour =  left 
    {
