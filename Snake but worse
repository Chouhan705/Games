        if(a[i][j+1]=='.')
        {
            a[i][j]=' ';
            t=a[i][++j];
        }
        else if(a[i-1][j]=='.')
        {
            a[i][j]=' ';
            t=a[--i][j];
        }
        else if(a[i][j-1]=='.')
        {
            a[i][j]=' ';
            t=a[i][--j];
        }
        else if(a[i+1][j]=='.')
        {
            a[i][j]=' ';
            a[++i][j]='.';
        }
        else
        {
            a[i][j]=' ';
            a[i][++j]='.';
        }
    }
    if(x==0 || y==0 || x==14 || y==14)
         {
             system("clear");
              for(int m=0;m<15;m++)
    {
        for(int r=0;r<15;r++)
        {
            printf("%c",a[m][r]);
        }
        printf("\n");
    }
    return printf("G A M E - O V E R !!!!!!!!");
         }
        return play();
    }
}
