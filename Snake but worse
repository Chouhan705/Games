    e=a[n][k];
   
    if(food())
    {
        if(a[i-1][j]=='.')
        {
            a[i+1][j]='.';
            t=a[++i][j];
        }
        else if(a[i][j-1]=='.')
        {
            a[i][j+1]='.';
            t=a[i][++j];
        }
        else if(a[i][j+1]=='.')
        {
            a[i][j-1]='.';
            t=a[i][--j];
