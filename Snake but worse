#include <stdio.h>
#include <stdlib.h>
char a[15][15];

int x=0,y=4,i=0,j=0,f;
int f1=8,f2=9,z1=0,z2;
char h,t;
int v[300];

int check()
{
    v[0]=8;
    if(v[z1]==6){
        if(v[z1-1]==7){
            z1--;
        return 1;
        }
        else 
        return 0;
    }
    else if(v[z1]==7){
        if(v[z1-1]==6){
            z1--;
        return 1;
        }
        else 
        return 0;
    }
    else if(v[z1]==8){
        if(v[z1-1]==9){
            z1--;
        return 1;
        }
        else 
        return 0;
    }
    else if(v[z1]==9){
        if(v[z1-1]==8){
            z1--;
            
        return 1;
        }
        else 
        return 0;
    }
}
int food()
{ 
    a[f1][f2]='a';
    system("clear");
              for(int m=0;m<15;m++)
    {
        for(int r=0;r<15;r++)
        {
            printf("%c",a[m][r]);
        }
        printf("\n");
    }
    if(x==f1 && y==f2)
    {
        if(f1==8 || f2==9){
          f1=3;
          f2=2;
        }
        else if(f1==3 || f2==2){
            f1=13;
            f2=13;
        }
        else if(f1==13 || f2==13){
            f1=7;
            f2=2;
        }
        else if(f1==7 || f2==2){
            f1=3;
            f2=10;
        }
        else if(f1==3 || f2==10){
            f1=13;
            f2=3;
        }
        else{
            f1=8;
            f2=9;
        }
        
           
        return 1; 
        }
        
       
    
    else
    {
        return 0;
    }
    
}

int up()
{
