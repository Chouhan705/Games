int play()
{
    
    system("clear");
     for(int m=0;m<15;m++)
    {
        for(int r=0;r<15;r++)
        {
            printf("%c",a[m][r]);
        }
        printf("\n");
    }
    scanf("%d",&f);
    z1++;
    v[z1]=f;
    switch(f)
    {
case 6:
      
    
     if(check())
     return play();
     
     up();
      
     break;
    
 case 7:
     if(check())
     return play();
     down();
     break;

 case 8:
       if(check())
       return play();
      rit();
      break;

case 9:
       if(check())
       return play();
      left();
      break;
}
}
void main()
{
    int m,r;
    for(int m=0;m<15;m++)
    {
        for(int r=0;r<15;r++)
        {
            if(m==0 || r==0 || m==14 || r==14)
            a[m][r]='*';
            else
            a[m][r]=' ';
        }
    }
    a[f1][f2]='a';
    a[1][1]='.';
    a[1][2]='.';
    a[1][3]='.';
    a[1][4]='.';
    a[1][5]='.';
    h=a[++x][++y];
    
    t=a[++i][++j];
    play();
    
    
}
